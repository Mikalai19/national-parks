<form method="POST" action="/parks">
  <label for="name">Name</label>
  <input type="text" name="name" />

  <label for="about">About</label>
  <input id="about" type="text" name="about" />

  <input type="submit" />
</form>

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All Parks</title>
  </head>
  <body>
    <% for( let i = 0; i < parks.length; i++ ) { %> <% let a = parks[i]; %> <% a
    = a.toJSON() %> <% if(a.name === 'Glacier National Park'){%>
    <p><%= a.weatherInfo %></p>
    <% break; %> <% }%> <% }%>
  </body>
</html>
